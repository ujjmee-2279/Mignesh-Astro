  <script>

    const apiKey = import.meta.env.PUBLIC_ApiKey as string;

    const secretKey = import.meta.env.PUBLIC_SecretKey as string;

    //dropdown date menu
    const refineryDateSection = document.getElementById("date-picker-section");
    const refineryDate = document.getElementById("date-picker");
    refineryDateSection.addEventListener("click", () => {
      if (refineryDate.classList.contains("hidden")) {
        refineryDate.classList.remove("hidden");
      } else {
        refineryDate.classList.add("hidden");
      }
    });

    // All the refinery Published Data API
    const refinary_all_data = await fetch(
      // `https://mignesh.frappe.cloud/api/resource/Refinery%20Price%20List?filters=[[%22publish%22,%22=%22,1]]&fields=[%22refinery_name%22,%22price_list%22,%22price_list_image%22,%22custom_name%22,%22check_first%22,%22creation%22,%22name%22,%22from_date%22,%22month%22]`,
      `http://192.168.2.126:8001/api/resource/Refinery%20Price%20List?filters=[[%22publish%22,%22=%22,1]]&fields=[%22refinery_name%22,%22price_list%22,%22price_list_image%22,%22custom_name%22,%22check_first%22,%22creation%22,%22name%22,%22from_date%22,%22month%22]`,
      {
        method: "GET",
        headers: {
          Authorization: `token ${apiKey}:${secretKey}`,
        },

      }
    );
    const refinary_object = await refinary_all_data.json();
    const refinary_data = refinary_object.data;
    // console.log(refinary_data);

    // all the rifinery Brand Data API
    const refinary_list_json = await fetch(
      // `https://mignesh.frappe.cloud/api/resource/Brand`,
      `http://192.168.2.126:8001/api/resource/Brand`,
      {
        method: "GET",
        headers: {
          Authorization: `token ${apiKey}:${secretKey}`,
        },

      }
    );
    const refinary_list_data = await refinary_list_json.json();
    const refinary_list = refinary_list_data.data;

    // For the Filter all The data on the bases of Refinery name
    const jsonData = refinary_data;
    // console.log(jsonData);
    const que = refinary_list;
    // console.log(que)
    const subCategoryData = {};

    for (const item of que) {
      const name = item.name;
      const temp = [];
      const blank = [];
      for (const dataItem of jsonData) {
        // console.log(dataItem)
        if (dataItem.refinery_name === name) {
          // console.log(name);
          temp.push(dataItem);
        }
      }
      // console.log(temp)

      // if the array is empty then dont append it to the object
      if (temp.length === 0) {
        // console.log("HI")
      } else {
        subCategoryData[name] = temp;
      }
    }
    // console.log(subCategoryData);

    //for the sorting refinery as per the from date
    var refinery_select_data = [];
    var date_refinery_data = [];
    for (const item in subCategoryData) {
      // console.log(subCategoryData[item]);
      const data = subCategoryData[item];
      const filteredData = data.filter((item) => item.from_date);

      // Sort the filtered data in ascending order based on from_date
      filteredData.sort((b, a) => a.from_date.localeCompare(b.from_date));
      refinery_select_data.push(filteredData[0]);
      // console.log(filteredData[0]);
      // console.log(filteredData);
      filteredData.forEach((element) => {
        date_refinery_data.push(element);
      });
    }

    //On load
    // select element for the pdf view
    const refinerySelect = document.getElementById("refinery-select");
    refinery_select_data.forEach((data) => {
      const option = document.createElement("option");
      option.value = data.refinery_name;
      option.innerText = data.refinery_name;
      option.classList.add("h");
      refinerySelect.appendChild(option);
    });
    var selectedValue = (refinerySelect as HTMLSelectElement).value;
    var refineryHead = document.getElementById("refinery-head");
    refineryHead.innerText = (refinerySelect as HTMLSelectElement).value;

    //load pdf as per the select field
    refinery_select_data.forEach((data) => {
      // console.log(data.refinery_name)
      if (data.refinery_name == selectedValue) {
        const pdfView = document.getElementById("pdf-view");
        const pdfImage = document.getElementById("pdf-image");
        pdfView.addEventListener("load", function () {
          pdfView.style.height = "650px";
          // console.log('PDF viewer has been forcefully loaded.');
        });

        const lastUdated = document.getElementById("last-updated");
        lastUdated.innerText =
          "Last Updated on " +
          data.month +
          " " +
          data.from_date.slice(8, 11) +
          ", " +
          data.from_date.slice(0, 4);

        // for pdf element
        (pdfView as HTMLObjectElement).data =
          "https://www.migneshglobal.com" + data.price_list + "#view=FitH";

        // for image element
        (pdfImage as HTMLImageElement).src =
          "https://www.migneshglobal.com" + data.price_list_image;

        // for download button
        const DownloadBtn = document
          .getElementById("download-btn")
          .querySelector(".btn-6") as HTMLAnchorElement;
        DownloadBtn.href = "https://www.migneshglobal.com" + data.price_list;
        DownloadBtn.download = data.price_list.slice(7);
      }
    });

    // for the date picker dropdown
    const MonthPicker_div = document.getElementById("month-picker");
    MonthPicker_div.innerHTML = "";
    // console.log(MonthPicker_div);

    // for rendering the data as per the record
    date_refinery_data.forEach((element) => {
      if (element.refinery_name == selectedValue) {
        // console.log(element);
        var temp = "";
        if (element.check_first == 1) {
          temp = " 1st Half";
        } else {
          temp = " 2nd Half";
        }
        var innerP = document.createElement("p");
        innerP.addEventListener("click", () => {
          const pdfView = document.getElementById("pdf-view");
          const pdfImage = document.getElementById("pdf-image");
          // for pdf
          if (
            (pdfView as HTMLObjectElement).data !=
            "https://www.migneshglobal.com" + element.price_list + "#view=FitH"
          ) {
            (pdfView as HTMLObjectElement).data =

              "https://www.migneshglobal.com" +
              element.price_list +
              "#view=FitH";
          }
          // for image
          (pdfImage as HTMLImageElement).src =
            "https://www.migneshglobal.com" + element.price_list_image;
          // for download button
          const DownloadBtn = document
            .getElementById("download-btn")
            .querySelector(".btn-6") as HTMLAnchorElement;

          DownloadBtn.href =
            "https://www.migneshglobal.com" + element.price_list;

          DownloadBtn.download = element.price_list.slice(7);
        });
        innerP.innerText = element.custom_name;
        innerP.classList.add("bg-[#ffffff]", "py-1", "px-2", "rounded-lg");
        MonthPicker_div.appendChild(innerP);
      }
    });

    //Change event on the selectbox
    refinerySelect.addEventListener("change", function () {
      // hide the dropdown menu as the select changes
      refineryDate.classList.add("hidden");

      var selectedValue = (refinerySelect as HTMLSelectElement).value;
      var options = refinerySelect.querySelectorAll("option");

      // for the options of the selectbox
      options.forEach(function (option) {
        if (option.value === selectedValue) {
          refineryHead.innerText = selectedValue;
          refinery_select_data.forEach((data) => {
            if (data.refinery_name == selectedValue) {
              const lastUdated = document.getElementById("last-updated");
              lastUdated.innerText =
                "Last Updated on " +
                data.month +
                " " +
                data.from_date.slice(8, 11) +
                ", " +
                data.from_date.slice(0, 4);
              const pdfView = document.getElementById("pdf-view");
              const pdfImage = document.getElementById("pdf-image");

              // for download button
              const DownloadBtn = document
                .getElementById("download-btn")
                .querySelector(".btn-6") as HTMLAnchorElement;

              DownloadBtn.href =
                "https://www.migneshglobal.com" + data.price_list;

              DownloadBtn.download = data.price_list.slice(7);

              // for pdf
              (pdfView as HTMLObjectElement).data =

                "https://www.migneshglobal.com" +
                data.price_list +
                "#view=FitH";


              // for image
              (pdfImage as HTMLImageElement).src =
                "https://www.migneshglobal.com" + data.price_list_image;
            }
          });
        }
      });

      // for the datepicker dropdown
      const MonthPicker_div = document.getElementById("month-picker");
      MonthPicker_div.innerHTML = "";

      // for rendering the data as per the record
      date_refinery_data.forEach((element) => {
        if (element.refinery_name == selectedValue) {
          // console.log(element);
          var temp = "";
          if (element.check_first == 1) {
            temp = " 1st Half";
          } else {
            temp = " 2nd Half";
          }
          var innerP = document.createElement("p");
          innerP.addEventListener("click", () => {
            const pdfView = document.getElementById("pdf-view");
            const pdfImage = document.getElementById("pdf-image");
            // for pdf
            if (
              (pdfView as HTMLObjectElement).data !=

              "https://www.migneshglobal.com" +
                element.price_list +
                "#view=FitH"
            ) {
              (pdfView as HTMLObjectElement).data =
                "https://www.migneshglobal.com" +
                element.price_list +
                "#view=FitH";
            }
            // for image
            (pdfImage as HTMLImageElement).src =
              "https://www.migneshglobal.com" + element.price_list_image;
            // for download button
            const DownloadBtn = document
              .getElementById("download-btn")
              .querySelector(".btn-6") as HTMLAnchorElement;

            DownloadBtn.href =
              "https://www.migneshglobal.com" + element.price_list;

            DownloadBtn.download = element.price_list.slice(7);
          });
          innerP.innerText = element.custom_name;
          innerP.classList.add("bg-[#ffffff]", "py-1", "px-2", "rounded-lg");
          MonthPicker_div.appendChild(innerP);
        }
      });
    });
  </script>

