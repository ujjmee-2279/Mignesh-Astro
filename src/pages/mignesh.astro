---
import Layout from "../layouts/Layout.astro";
import Why from "../components/Why.astro";
import Product from "../components/Product.astro";
import Faq from "../components/Faq.astro";
import Process from "../components/Process.astro";
import Button from "../components/Button.astro";
import Ourteam from "../components/Ourteam.astro";
---


<Layout
  title="Mignesh Global"
  meta_title="NHAI Approved Bitumen, Lubricants MAnufacturer adn Supplier - Mignesh Global"
  meta_desc="Mignesh Global supplies top quality industrial oils, bitumen, lubricants, emulsion and more catering to construction, manufacturing and infrastructure sectors."
>
  <main>
    <!-- slider section  -->
    <section class="slider bg-[#D9D9D9] relative overflow-hidden">
      <!-- Image 1 -->
      <div
        class="slide-1 sm:h-[60vh] md:h-[80vh] ssm:min-h-[328px] ssm:max-h-[360px] md:min-h-[400px] md:max-h-[550px] lg:h-screen lg:max-h-[701px] object-cover relative md:ml-[30vw] lg:ml-[15vw] xl:ml-[5vw] 2xl:ml-0"
      >
        <img
          class="block ssm:hidden h-full w-auto msm:w-full object-cover"
          src="../../public/images/index/slide_1_2.webp"

          alt="Image"
        />
        <img
          class="h-full absolute top-0 left-[30vh] max-w-none object-cover object-center md:object-left hidden ssm:block md:hidden"
          src="../../public/images/index/slide_1_1.webp"

          alt="Image"
        />
        <img
          class="hidden md:block h-full object-cover object-left md:ml-auto absolute top-0 right-0"
          src="../../public/images/index/slide_1.webp"

          alt="Image"
        />
        <div
          class="absolute hidden ssm:block max-w-[150px] md:max-w-[180px] bottom-5 left-[42vh] md:left-auto w-[35%] md:right-auto md:bottom-10 lg:bottom-20 lg:max-w-[240px] xl:max-w-[300px] 2xl:max-w-max lg:w-auto xl:right-[950px] 2xl:left-auto 2xl:right-[967px]"
        >

          <img src="../../public/images/index/drum.webp" alt="Image" />
        </div>
      </div>

      <!-- Image 2 -->
      <div
        class="slide-2 hidden sm:h-[60vh] md:h-[80vh] ssm:min-h-[328px] ssm:max-h-[360px] md:min-h-[400px] md:max-h-[550px] lg:h-screen lg:max-h-[701px] object-cover relative md:ml-[30vw] lg:ml-[15vw] xl:ml-[5vw] 2xl:ml-0"
      >
        <img
          class="block ssm:hidden h-full w-auto msm:w-full object-cover"
          src="../../public/images/index/slide_2_2.webp"

          alt="Image"
        />
        <img
          class="h-full absolute top-0 left-[30vh] max-w-none object-cover object-center md:object-left hidden ssm:block md:hidden"
          src="../../public/images/index/slide_2_1.webp"

          alt="Image"
        />
        <img
          class="hidden md:block h-full object-cover object-left md:ml-auto absolute top-0 right-0"
          src="../../public/images/index/slide_2.webp"

          alt="Image"
        />
        <div
          class="absolute hidden ssm:block max-w-[150px] md:max-w-[180px] bottom-5 left-[42vh] md:left-auto w-[35%] md:right-auto md:bottom-10 lg:bottom-20 lg:max-w-[240px] xl:max-w-[300px] 2xl:max-w-max lg:w-auto xl:right-[950px] 2xl:left-auto 2xl:right-[967px]"
        >

          <img src="../../public/images/index/truck.webp" alt="Image" />
        </div>
      </div>

      <!-- Image 3 -->
      <div
        class="slide-3 hidden sm:h-[60vh] md:h-[80vh] ssm:min-h-[328px] ssm:max-h-[360px] md:min-h-[400px] md:max-h-[550px] lg:h-screen lg:max-h-[701px] object-cover relative md:ml-[30vw] lg:ml-[15vw] xl:ml-[5vw] 2xl:ml-0"
      >
        <img
          class="block ssm:hidden h-full w-auto msm:w-full object-cover"
          src="../../public/images/index/slide_3_2.webp"

          alt="Image"

        />
        <img
          class="h-full absolute top-0 left-[30vh] max-w-none object-cover object-center md:object-left hidden ssm:block md:hidden"
          src="../../public/images/index/slide_3_1.webp"

          alt="Image"
        />
        <img
          class="hidden md:block h-full object-cover object-left md:ml-auto absolute top-0 right-0"
          src="../../public/images/index/slide_3.webp"

          alt="Image"
        />
        <div
          class="absolute hidden ssm:block max-w-[150px] md:max-w-[180px] bottom-5 left-[42vh] md:left-auto w-[35%] md:right-auto md:bottom-10 lg:bottom-20 lg:max-w-[240px] xl:max-w-[300px] 2xl:max-w-max lg:w-auto xl:right-[950px] 2xl:left-auto 2xl:right-[967px]"
        >

          <img src="../../public/images/index/oil.webp" alt="Image" />
        </div>
      </div>

      <!-- Image 4 -->
      <div
        class="slide-4 hidden sm:h-[60vh] md:h-[80vh] ssm:min-h-[328px] ssm:max-h-[360px] md:min-h-[400px] md:max-h-[550px] lg:h-screen lg:max-h-[701px] object-cover relative md:ml-[30vw] lg:ml-[15vw] xl:ml-[5vw] 2xl:ml-0"
      >
        <img
          class="block ssm:hidden h-full w-auto msm:w-full object-cover"
          src="../../public/images/index/slide_4_2.webp"

          alt="Image"

        />
        <img
          class="h-full absolute top-0 left-[30vh] max-w-none object-cover object-center md:object-left hidden ssm:block md:hidden"
          src="../../public/images/index/slide_4_1.webp"

          alt="Image"
        />
        <img
          class="hidden md:block h-full object-cover object-left md:ml-auto absolute top-0 right-0"
          src="../../public/images/index/slide_4.webp"

          alt="Image"
        />
        <div
          class="absolute hidden ssm:block max-w-[150px] md:max-w-[180px] bottom-5 left-[42vh] md:left-auto w-[35%] md:right-auto md:bottom-10 lg:bottom-20 lg:max-w-[240px] xl:max-w-[300px] 2xl:max-w-max lg:w-auto xl:right-[950px] 2xl:left-auto 2xl:right-[967px]"
        >

          <img src="../../public/images/index/bricks.webp" alt="Image" />

        </div>
      </div>

      <!-- Slider Caption  -->
      <div class="txt md:w-full ssm:absolute left-0 top-0">
        <div class="w-fit md:w-auto container-1">
          <div
            class="caption py-4 ssm:p-4 sm:p-8 md:px-0 md:py-16 ssm:max-w-[350px] sm:max-w-[400px]"
          >
            <h2
              class="text-2xl md:text-[40px] md:leading-[48px] text-[#48389C] uppercase md:w-[386px]"
            >
              <span class="normal-case text-xl md:text-[32px]">
                Welcome to
                <br />
              </span>
              <span class="text-3xl md:text-[48px] font-bold">Mignesh</span>

              Global!

            </h2>
            <p
              class="text-base md:text-[18px] md:leading-6 md:w-[414px] text-[#2C2C2C] my-2.5 md:mt-5 md:mb-6"
            >
              Your one-stop platform to purchase high-quality Infrastructure &
              Lubricant products such as Bitumen, Emulsion, Base Oil, Furnace
              Oil, etc.
            </p>

            <Button data="CHECK PRODUCTS" link="#product_section" />
          </div>
        </div>
      </div>

      <!-- Slider Buttons Container -->
      <div
        class="btns flex gap-3 flex-col h-[50vh] md:h-full left-[20px] lg:left-7 justify-center items-center absolute top-0"
      >

        <button
          class="btn-1 bg-[#E98135] w-[5px] h-[30px] rounded-[3px]"
          aria-label="Slider Buttons 1"></button>
        <button
          class="btn-2 bg-[#2E2C7090] w-[5px] h-[30px] rounded-[3px]"
          aria-label="Slider Buttons 2"></button>
        <button
          class="btn-3 bg-[#2E2C7090] w-[5px] h-[30px] rounded-[3px]"
          aria-label="Slider Buttons 3"></button>
        <button
          class="btn-4 bg-[#2E2C7090] w-[5px] h-[30px] rounded-[3px]"
          aria-label="Slider Buttons 4"></button>
      </div>
    </section>

    <!-- Why choose us section  -->
    <section
      class="why bg-gradient-to-r from-[#242D4A] to-[#4B5EAA] text-white"
    >
      <div
        class="container-1 flex flex-col lg:flex-row justify-between items-center py-8 lg:py-0"
      >
        <div class="item-1 lg:max-w-[524px] lg:w-[40%]">

          <img
            class=""
            src="../../public/images/index/why_main.webp"
            loading="lazy"
            alt="Image"
          />
          <p class="text-xl sm:text-[32px] mt-4 sm:mt-6 sm:leading-10">
            Get the best quality products at
          </p>
          <h2
            class="text-3xl ssm:text-[40px] sm:leading-[50px] sm:text-5xl font-bold"
          >
            pocket-friendly rates!
          </h2>
          <p class="text-base mt-4 sm:mt-8 lg:max-w-[463px]">
            Mignesh Global is the leading Company offering a wide range of
            products for all Infrastructure and Manufacturing companies looking
            to get high-quality products at the best rate and in a shorter time.
          </p>
        </div>

        <div class="item-2 w-full lg:w-[48%] mt-4 sm:mt-8 lg:mt-0 lg:py-[82px]">
          <div class="heading sm:mb-2">
            <h3 class="text-2xl lg:ml-[-16px]">Why Choose Us?</h3>
          </div>

          <!-- All Items  -->
          <div
            class="card flex flex-col ssm:flex-row sm:flex-col gap-2.5 lg:gap-0"
          >
            <!-- First Row  -->
            <div
              class="card ssm:w-[50%] sm:w-full flex flex-col sm:flex-row items-center md:justify-between gap-2.5 lg:gap-0"
            >
              <Why
                source="../../public/images/index/why_1.svg"
                body="One-stop platform for a variety of products!"
              />
              <Why
                source="../../public/images/index/why_4.svg"
                body="Fast Distribution with Mignesh Logistics."
              />
              <Why
                source="../../public/images/index/why_6.svg"
                body="End-to-End Customer Support."
              />
            </div>

            <!-- Second Row  -->
            <div
              class="card ssm:w-[50%] sm:w-full flex flex-col sm:flex-row items-center md:justify-between gap-2.5 lg:gap-0"
            >
              <Why
                source="../../public/images/index/why_2.svg"
                body="Best Market <br class='hidden ssm:block'> Rates."
              />
              <Why
                source="../../public/images/index/why_3.svg"
                body="Free <br class='hidden ssm:block'> Samples."
              />
              <Why
                source="../../public/images/index/why_5.svg"
                body="High-quality, <br class='hidden ssm:block'> trusted products."
              />
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- product section  -->
    <section class="products" id="product_section">
      <div class="products container-1 py-8 sm:py-14 lg:py-20">
        <!-- Heading  -->
        <div class="head-txt text-center md:text-left">

          <h3 class="text-[#EA8135] font-bold text-lg sm:text-xl md:leading-7">
            Browse through our diverse
          </h3>
          <h3
            class="text-[#48389C] text-2xl sm:text-[32px] md:leading-7 font-bold uppercase"
          >
            range of products
          </h3>
        </div>

        <!-- All Products  -->
        <div
          class="products w-full grid md:grid-cols-2 xl:grid-cols-3 gap-[20px] md:gap-[26px] justify-center md:justify-between mt-4 sm:mt-7 md:mt-[30px] lg:mt-[60px] m-auto"
        >
          <Product
            title="Bitumen & Bitumen Products"
            ,
            body1="Product categories overview text here lorem Ipsum is simply dummy text of the printing and industry. Lorem Ipsum has been the industry's standard dummy text"
            body2="Some product name goes here product 01, product 02, Bitumen VG 40, Bitumen VG 10, LDS"
            ,
            source="../../public/images/index/product_1.webp"
            btn="Bitumen & Bitumen Products"
            link="/bitumen"
          />

          <Product
            title="Lubricant Raw Material"
            ,
            body1="Product categories overview text here lorem Ipsum is simply dummy text of the printing and industry. Lorem Ipsum has been the industry's standard dummy text"
            body2="Some product name goes here product 01, product 02, Bitumen VG 40, Bitumen VG 10, LDS"
            ,
            source="../../public/images/index/product_2.webp"
            btn="Lubricant Raw Material"
            link="/lubricant"
          />

          <Product
            title="<p>Petroleum Products</p>"
            ,
            body1="Product categories overview text here lorem Ipsum is simply dummy text of the printing and industry. Lorem Ipsum has been the industry's standard dummy text					"
            body2="Some product name goes here product 01, product 02, Bitumen VG 40, Bitumen VG 10, LDS"
            ,
            source="../../public/images/index/product_4.webp"
            btn="Petroleum Products"
            link="/petroleum"
          />

        </div>
      </div>
    </section>

    <!-- ordering section  -->
    <section class="ordering bg-[#F9F9FB] overflow-hidden">
      <div
        class="ordering container-1 py-8 sm:py-14 lg:py-20 lg:pb-[117px] xl:pb-[145px] 2xl:pb-[90px]"
      >
        <!-- Heading  -->
        <div class="head text-center lg:text-left">

          <h2 class="text-[#EA8135] font-bold text-lg sm:text-xl md:leading-7">
            5-step easy
          </h2>
          <h3
            class="text-[#48389C] text-2xl sm:text-[32px] md:leading-7 font-bold uppercase"
          >
            ordering process!
          </h3>
        </div>

        <!-- All Process  -->
        <div
          id="bg-union"
          class="pl-4 lg:pl-0 lg:bg-[url(../../../../../public/images/index/union.webp)] bg-no-repeat bg-contain xl:mx-20 gap-5 mt-4 sm:mt-5 lg:mt-0"
        >
          <div
            class="lg:flex flex-col lg:flex-row justify-center relative lg:bottom-[9px] xl:bottom-[9px] 2xl:bottom-[38px] lg:gap-6 xl:gap-8 2xl:gap-16 2xl:ml-20 lg:left-[100px] xl:left-0"
          >
            <Process
              title="Choose the right products."
              ,
              body="You can browse the range of products from our website and choose as per your requirements."
              source1="../../public/images/index/process_1.webp"
              ,
              source2="../../public/images/index/1.svg"
            />
            <Process
              title="Reach out to us!"
              ,
              body="You can reach out to (91) 96014 76223 and place your order with us."
              source1="../../public/images/index/process_2.webp"
              ,
              source2="../../public/images/index/2.svg"
            />
          </div>

          <div
            class="lg:flex flex-col lg:flex-row-reverse justify-around relative lg:bottom-[-31px] xl:bottom-[-40px] 2xl:bottom-[-11px] lg:gap-5 2xl:gap-10"
          >
            <Process
              title="Sample checking!"
              ,
              body="To analyze the quality of our products, we will send you some of the pictures and report for the products via Whatsapp or email. If you want, we can even send the sample to you before placing an order."
              source1="../../public/images/index/process_3.webp"
              ,
              source2="../../public/images/index/3.svg"
            />
            <Process
              title="Final Payment"
              ,
              body="If you find everything well, you can make a final payment for the confirmation of your order."
              source1="../../public/images/index/process_4.webp"
              ,
              source2="../../public/images/index/4.svg"
            />
            <Process
              title="Seamless Tracking"
              ,
              body="The process is not over yet! After making final payment, you can even track your products status through our end-to-end GPS tracking."
              source1="../../public/images/index/process_5.webp"
              ,
              source2="../../public/images/index/5.svg"
            />
          </div>
        </div>
      </div>
    </section>

    <!-- client section  -->
    <section class="client" id="client_section">
      <div class="client container-1 py-8 sm:py-14 lg:py-20">
        <!-- Heading  -->
        <div class="head text-center md:text-left">

          <h2 class="text-[#EA8135] font-bold text-lg sm:text-xl md:leading-7">
            Our
          </h2>
          <h3
            class="text-[#48389C] text-2xl sm:text-[32px] md:leading-7 font-bold uppercase"
          >
            Esteemed Clients!
          </h3>
        </div>

        <!-- Client Slider Jquery -->
        <div class="customer-logos w-full slider mt-4 sm:mt-6 md:mt-12">

          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/Of_Business.webp"
              alt="Image"
            />
          </div>

          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/HK RMC Logo.webp"
              alt="Image"
            />
          </div>
          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/VR_PetroChem.webp"
              alt="Image"
            />
          </div>

          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/Sabar_dairy.webp"
              alt="Image"
            />
          </div>


          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/Maruti-LOGO.webp"
              alt="Image"
            />
          </div>



          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/vevelon_logo.webp"
              alt="Image"
            />
          </div>
          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/Vijay_Construction.webp"
              alt="Image"
            />
          </div>



          <div class="slide">
            <img
              decoding="async"
              src="../../public/images/index/Pertners_Logo/Zippmat.webp"
              alt="Image"
            />
          </div>


        </div>
      </div>
    </section>

    <!--refinery section-->
    <section class="refinery bg-[#F9F9FB]">
      <div class="refinery container-1 py-8 sm:py-14 lg:py-20">
        <div
          class="head-content flex flex-col md:flex-row gap-4 md:gap-0 justify-between"
        >
          <!-- Heading  -->
          <div class="head text-center md:text-left">
            <!-- <h6
              class="text-[#EA8135] font-bold text-lg sm:text-xl md:leading-7"
            >
              Bitumen
            </h6> -->
            <h2
              class="text-[#48389C] text-2xl sm:text-[32px] md:leading-7 font-bold uppercase"
            >
              Price IN INDIA
            </h2>
          </div>

          <!-- Select Box  -->
          <div
            class="selectbox w-full md:max-w-[300px] lg:max-w-[440px] bg-white"
          >
            <select
              name="refinery"

              aria-label="select refinery"

              id="refinery-select"
              class="border rounded-lg w-full p-2 sm:p-4 font-bold border-slate-300"
            >
            </select>
          </div>
        </div>

        <!-- PDF Section  -->
        <div class="mt-8 md:mt-12">
          <div
            class="thead bg-[#5A6079] text-white flex flex-col md:flex-row justify-between gap-2.5 border-white"
          >
            <h3
              class="text-lg md:text-2xl md:leading-6 font-bold p-2.5 sm:p-[14px]"
              id="refinery-head"
            >
            </h3>
            <div
              id="date-picker-section"
              class="font-semibold text-base flex items-center md:items-start lg:items-center gap-2.5 cursor-pointer select-none p-2.5 sm:p-[14px]"
            >
              <div class="calendar-icon relative">
                <svg
                  width="16"
                  height="16"
                  viewBox="0 0 16 16"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    d="M2.12149 16H13.2859C14.4563 16 15.4074 15.0489 15.4074 13.8785V3.30665C15.4074 2.13628 14.4563 1.18516 13.2859 1.18516H12.4445V0.592582C12.4445 0.266652 12.1778 0 11.8518 0C11.5259 0 11.2592 0.266652 11.2592 0.592582V1.18516H4.14817V0.592582C4.14817 0.266652 3.88147 0 3.55554 0C3.22961 0 2.96296 0.266652 2.96296 0.592582V1.18516H2.12149C0.951119 1.18516 0 2.13628 0 3.30665V13.8785C0 15.0489 0.951119 16 2.12149 16ZM1.18521 3.30665C1.18521 2.79111 1.60595 2.37038 2.12149 2.37038H2.96296V2.96296C2.96296 3.28889 3.22961 3.55554 3.55554 3.55554C3.88147 3.55554 4.14812 3.28889 4.14812 2.96296V2.37038H11.2592V2.96296C11.2592 3.28889 11.5259 3.55554 11.8518 3.55554C12.1778 3.55554 12.4444 3.28889 12.4444 2.96296V2.37038H13.2859C13.8014 2.37038 14.2222 2.79111 14.2222 3.30665V5.03704H1.18521V3.30665ZM1.18521 6.22221H14.2223V13.8785C14.2223 14.3941 13.8015 14.8148 13.286 14.8148H2.12149C1.60595 14.8148 1.18521 14.3941 1.18521 13.8785V6.22221Z"
                    fill="white"></path>
                </svg>

                <div
                  id="date-picker"
                  class="date-picker hidden bg-[#e4e4e4] w-[270px] py-2.5 max-h-[270px] overflow-y-auto top-[34px] absolute text-black rounded-lg shadow-[0px_0px_25px_5px_rgba(0,0,0,0.1)]"
                >
                  <div
                    id="month-picker"
                    class="it w-[80%] m-auto flex flex-col gap-2 py-1 text-center"
                  >
                  </div>
                </div>
              </div>
              <span id="last-updated"></span>
              <svg
                class="rotate-90"
                width="9"
                height="12"
                viewBox="0 0 9 12"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  fill-rule="evenodd"
                  clip-rule="evenodd"
                  d="M1.917 12.009L0 10.122L4.182 6.005L0 1.887L1.917 0L8.014 6.005L1.917 12.009Z"
                  fill="white"></path>
              </svg>
            </div>
          </div>

          <div
            class="ref-table overflow-scroll lg:overflow-hidden max-h-[550px] max-w-[100%] lg:max-h-[650px]"
          >
            <object
              id="pdf-view"
              class="pdf-view"
              data=""
              type="application/pdf"
              width="100%"
              height="650"
            >
              <img id="pdf-image" class="object-cover" src="" alt="" />
            </object>
          </div>
          <div class="w-fit mt-8 mx-auto" id="download-btn">
            <Button data="Download Price List PDF" link="" />
          </div>
        </div>
      </div>
    </section>

    <!-- CTA source section  -->
    <section class="source bg-gradient-to-r from-[#242D4A] to-[#4B5EAA]">
      <div class="source container-1">
        <div
          class="py-8 md:py-14 gap-8 flex flex-col-reverse md:flex-row justify-between items-center text-white"
        >
          <!-- CTA Text  -->
